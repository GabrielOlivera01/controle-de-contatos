@model ControleDeUsuarios.Models.Usuario

@{
    ViewData["Title"] = "Create";
}

<h4>Adicionar Contato</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
        
            @Html.Partial("_FormUsuario")

            <div class="form-group">
                <input type="submit" value="Cadastrar" class="btn btn-primary mt-3"/>
                <button class="btn btn-secondary mt-3">
                    <a asp-action="Index" class="text-light text-decoration-none">Voltar</a>
                </button>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
